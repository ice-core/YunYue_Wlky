<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:loonggg="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#FFFFFF"
    android:orientation="vertical">

    <LinearLayout
        android:id="@+id/ll_fjsc_shouye_sjlb_item"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginLeft="15dp"
            android:layout_marginRight="15dp">

            <ImageView
                android:id="@+id/iv_fjsc_shouye_sjlb_logo"
                android:layout_width="55dp"
                android:layout_height="55dp"
                android:layout_marginTop="13dp" />

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginLeft="10dp"
                android:layout_marginTop="10dp"
                android:gravity="center_vertical"
                android:orientation="vertical">

                <TextView
                    android:id="@+id/tv_fjsc_shouye_sjlb_shangjianame"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:singleLine="true"
                    android:text=""
                    android:textColor="@color/black"
                    android:textSize="14sp" />

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="5dp"
                    android:gravity="center_vertical">

                    <ImageView
                        android:id="@+id/iv_fjsc_splb_pj1"
                        android:layout_width="7dp"
                        android:layout_height="7dp"
                        android:src="@drawable/star_selected" />

                    <ImageView
                        android:id="@+id/iv_fjsc_splb_pj2"
                        android:layout_width="7dp"
                        android:layout_height="7dp"
                        android:layout_marginLeft="2dp"
                        android:src="@drawable/star_selected" />

                    <ImageView
                        android:id="@+id/iv_fjsc_splb_pj3"
                        android:layout_width="7dp"
                        android:layout_height="7dp"
                        android:layout_marginLeft="2dp"
                        android:src="@drawable/star_selected" />

                    <ImageView
                        android:id="@+id/iv_fjsc_splb_pj4"
                        android:layout_width="7dp"
                        android:layout_height="7dp"
                        android:layout_marginLeft="2dp"
                        android:src="@drawable/star_selected" />

                    <ImageView
                        android:id="@+id/iv_fjsc_splb_pj5"
                        android:layout_width="7dp"
                        android:layout_height="7dp"
                        android:layout_marginLeft="2dp"
                        android:src="@drawable/star_selected" />

                    <TextView
                        android:id="@+id/tv_fjsc_shouye_sjlb_shangjia_fen"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="4dp"
                        android:text=""
                        android:textColor="#4A4A4A"
                        android:textSize="10sp" />

                    <TextView
                        android:id="@+id/tv_fjsc_shouye_sjlb_xiao_num"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="6dp"
                        android:text="月售"
                        android:textColor="#4A4A4A"
                        android:textSize="10sp" />

                    <TextView
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="1" />

                    <TextView
                        android:id="@+id/tv_fjsc_shouye_sjlb_duration"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text=""
                        android:textColor="#4A4A4A"
                        android:textSize="10sp" />

                    <TextView
                        android:layout_width="2dp"
                        android:layout_height="9dp"
                        android:layout_marginLeft="3dp"
                        android:layout_marginRight="3dp"
                        android:background="#E1E1E1" />

                    <TextView
                        android:id="@+id/tv_fjsc_shouye_sjlb_distance"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text=""
                        android:textColor="#4A4A4A"
                        android:textSize="10sp" />

                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="5dp"
                    android:gravity="center_vertical">

                    <TextView
                        android:id="@+id/tv_fjsc_shouye_sjlb_uptosend"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="起送 ¥"
                        android:textColor="#4A4A4A"
                        android:textSize="10sp" />

                    <TextView
                        android:layout_width="2dp"
                        android:layout_height="9dp"
                        android:layout_marginLeft="3dp"
                        android:layout_marginRight="3dp"
                        android:background="#E1E1E1"
                        android:visibility="gone" />

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="优惠配送 ¥1.3"
                        android:textColor="#4A4A4A"
                        android:textSize="10sp"
                        android:visibility="gone" />
                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="14dp"
                    android:layout_marginTop="10dp"
                    android:gravity="center_vertical">

                    <TextView
                        android:layout_width="30dp"
                        android:layout_height="match_parent"
                        android:background="@color/theme"
                        android:gravity="center"
                        android:text="服务"
                        android:textColor="@color/white"
                        android:textSize="9sp" />


                    <TextView
                        android:id="@+id/tv_fjsc_sjlb_fw1"
                        android:layout_width="wrap_content"
                        android:layout_height="match_parent"
                        android:layout_marginLeft="10dp"
                        android:background="@drawable/bk_e1e1e1_1"
                        android:gravity="center"
                        android:paddingLeft="4dp"
                        android:paddingRight="4dp"
                        android:text=""
                        android:textColor="#4A4A4A"
                        android:textSize="9sp" />


                    <TextView
                        android:id="@+id/tv_fjsc_sjlb_fw2"
                        android:layout_width="wrap_content"
                        android:layout_height="match_parent"
                        android:layout_marginLeft="10dp"
                        android:background="@drawable/bk_e1e1e1_1"
                        android:gravity="center"
                        android:paddingLeft="4dp"
                        android:paddingRight="4dp"
                        android:text=""
                        android:textColor="#4A4A4A"
                        android:textSize="9sp" />

                    <TextView
                        android:id="@+id/tv_fjsc_sjlb_fw3"
                        android:layout_width="wrap_content"
                        android:layout_height="match_parent"
                        android:layout_marginLeft="10dp"
                        android:background="@drawable/bk_e1e1e1_1"
                        android:gravity="center"
                        android:paddingLeft="4dp"
                        android:paddingRight="4dp"
                        android:text=""
                        android:textColor="#4A4A4A"
                        android:textSize="9sp" />

                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="15dp">

                    <LinearLayout
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:orientation="vertical">

                        <ImageView
                            android:id="@+id/tv_fjsc_shouye_sjlb_sp_logo1"
                            android:layout_width="match_parent"
                            android:layout_height="64dp"
                            android:scaleType="centerCrop" />

                        <TextView
                            android:id="@+id/tv_fjsc_shouye_sjlb_sp_name1"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="6dp"
                            android:singleLine="true"
                            android:text=""
                            android:textColor="#4A4A4A"
                            android:textSize="12sp" />

                        <TextView
                            android:id="@+id/tv_fjsc_shouye_sjlb_sp_price1"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="4dp"
                            android:singleLine="true"
                            android:text=""
                            android:textColor="#4A4A4A"
                            android:textSize="14sp" />
                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="15dp"
                        android:layout_weight="1"
                        android:orientation="vertical">

                        <ImageView
                            android:id="@+id/tv_fjsc_shouye_sjlb_sp_logo2"
                            android:layout_width="match_parent"
                            android:layout_height="64dp"
                            android:scaleType="centerCrop" />

                        <TextView
                            android:id="@+id/tv_fjsc_shouye_sjlb_sp_name2"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="6dp"
                            android:singleLine="true"
                            android:text=""
                            android:textColor="#4A4A4A"
                            android:textSize="12sp" />

                        <TextView
                            android:id="@+id/tv_fjsc_shouye_sjlb_sp_price2"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="4dp"
                            android:singleLine="true"
                            android:text=""
                            android:textColor="#4A4A4A"
                            android:textSize="14sp" />
                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="15dp"
                        android:layout_weight="1"
                        android:orientation="vertical">

                        <ImageView
                            android:id="@+id/tv_fjsc_shouye_sjlb_sp_logo3"
                            android:layout_width="match_parent"
                            android:layout_height="64dp"
                            android:scaleType="centerCrop" />

                        <TextView
                            android:id="@+id/tv_fjsc_shouye_sjlb_sp_name3"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="6dp"
                            android:singleLine="true"
                            android:text=""
                            android:textColor="#4A4A4A"
                            android:textSize="12sp" />

                        <TextView
                            android:id="@+id/tv_fjsc_shouye_sjlb_sp_price3"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="4dp"
                            android:singleLine="true"
                            android:text=""
                            android:textColor="#4A4A4A"
                            android:textSize="14sp" />
                    </LinearLayout>
                </LinearLayout>
            </LinearLayout>
        </LinearLayout>

    </LinearLayout>

    <TextView
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:layout_marginTop="15dp"
        android:background="#F5F5F5" />

</LinearLayout>
